<!-- hour_header must be sorted based on hour
entry can only be only one of hour or day/month type
day_header entries have a higher priority then hour entries -->

<daylight_headers>
    <hour_header hour="6" image="aicp_sunrise" />
    <hour_header hour="9" image="aicp_morning" />
    <hour_header hour="12" image="aicp_noon" />
    <hour_header hour="14" image="aicp_afternoon" />
    <hour_header hour="19" image="aicp_sunset" />
    <hour_header hour="21" image="aicp_night" />
</daylight_headers>
